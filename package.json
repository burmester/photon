{
  "name": "Photon",
  "version": "0.0.0",
  "description": "Simple image editing app",
  "keywords": [
    "webpack",
    "express",
    "react",
    "redux",
    "http2",
    "progressive web app",
    "service worker",
    "mongodb",
    "socket",
    "graphql"
  ],
  "repository": "git@github.com:burmester/photon.git",
  "scripts": {
    "install": "npm install -g webpack webpack-dev-server http-server",
    "build": "babel-node --presets es2015 ./build.js",
    "dev": "babel-node --presets es2015 ./src/server/dev.js",
    "prod": "NODE_ENV=production PORT=3000 node ./private/build/server.js",
    "prod2": "NODE_ENV=production PORT=3000 USEHTTP2=true node ./private/build/server.js",
    "prestart": "babel-node --presets es2015 ./build.js",
    "start": "node ./private/build/server.js"
  },
  "author": "Johan Burmester <johanbur@kth.se",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.25.0",
    "eslint": "^3.0.1",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-react": "^2.2.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-react": "^5.0.0",
    "eslint-plugin-standard": "^1.3.1",
    "json-loader": "^0.5.4",
    "node-sass": "^3.10.0",
    "react-hot-loader": "^3.0.0-beta.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack-dev-server": "^1.15.1",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-runtime": "^6.11.6",
    "compression": "^1.6.2",
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "express-graphql": "^0.5.4",
    "graphql": "^0.7.0",
    "moment": "^2.15.0",
    "mongodb": "^2.2.10",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "spdy": "^3.3.5",
    "webpack": "^1.13.2",
    "whatwg-fetch": "^2.0.1"
  }
}
